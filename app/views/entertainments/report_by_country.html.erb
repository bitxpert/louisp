<div class="page-header">
  <h1>Report By Country</h1>
</div>
<%- model_class = Entertainment -%>
<table id="flip-scroll" class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:country) %></th>
      <th class="numeric">Count</th>
    </tr>
  </thead>
  <tbody>
    <% @by.each do |entertainment| %> 
      <tr>
        <td data-title="Country %" class="numeric"><%= link_to Carmen::Country.coded(entertainment).name,'/entertainments/report/'+entertainment+'/1/1/1/', :class => 'undecorate'  %></td>
        <td data-title="Url" class="numeric"><%= @count[entertainment] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
