<% javascript_include_tag "entertainments_of_DAP.js" %>
<%- model_class = Entertainment -%>
<div class="page-header">
  <h1><%=t '.title', :default => "DAP "+model_class.model_name.human.pluralize.titleize %></h1>
</div>


<table id="entertainments" class="table table-striped edit" data-source="<%= dap_entertainments_entertainments_url(format: "json") %>">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= "EE "+model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:city) %></th>
      <th><%= model_class.human_attribute_name(:url) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th>DAP Name</th>
      <th>Action</th>
    </tr>
  </thead>
</table>

